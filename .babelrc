{
    "presets": [
        ["@babel/preset-env", {"modules":false}],
        "@babel/preset-react"
    ],
    "plugins": [
        "transform-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true, // or 'css'
            },
            "ant"
        ],
        [
            "import",
            {
                "libraryName": "lodash",
                "libraryDirectory": "",
                "camel2DashComponentName": false
            },
            "lodash"
        ]
